{
  "name": "tokamak-zk-rollup-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next node_modules/.cache",
    "postinstall": "mkdir -p data && echo 'Local database directory created'",
    "db:reset": "rm -rf data/local-db.json && echo 'Database reset'",
    "test": "npm run test:contracts",
    "test:contracts": "cd contracts && forge test -vvv",
    "test:contracts:gas": "cd contracts && forge test -vvv --gas-report",
    "test:e2e:local": "npx tsx scripts/e2e-test-local.ts",
    "test:e2e:sepolia": "npx tsx scripts/e2e-test-sepolia.ts",
    "test:all": "npm run test:contracts && npm run test:e2e:local"
  },
